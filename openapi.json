{
    "openapi": "3.0.0",
    "info": {
        "title": "RealmAPI",
        "description": "A small api to support the Realm Enviroment",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://win19-benserver:527/api/"
        },
        {
            "url": "http://localhost:527/api/"
        }
    ],
    "paths": {
        "/{version}/games/deadbydaylight/shrine/:time": {
            "get": {
                "summary": "Returns the shrine active at that time",
                "description": "The shrine is a weekly rotation of teachable perks",
                "tags": [
                    "/games/deadbydaylight"
                ],
                "responses": {
                    "200": {
                        "description": "generator JSON object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "perks": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "format": "int32",
                                                        "minimum": 1
                                                    },
                                                    "name": {
                                                        "type": "string"
                                                    },
                                                    "owner": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{version}/games/deadbydaylight/shrine/all": {
            "get": {
                "summary": "Returns all shrines",
                "description": "The shrine is a weekly rotation of teachable perks",
                "tags": [
                    "/games/deadbydaylight"
                ],
                "responses": {
                    "200": {
                        "description": "generator JSON object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "start_time": {
                                                "type": "integer",
                                                "format": "int32",
                                                "minimum": 1
                                            },
                                            "end_time": {
                                                "type": "integer",
                                                "format": "int32",
                                                "minimum": 1
                                            },
                                            "perks": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "id": {
                                                            "type": "integer",
                                                            "format": "int32",
                                                            "minimum": 1
                                                        },
                                                        "name": {
                                                            "type": "string"
                                                        },
                                                        "owner": {
                                                            "type": "string"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{version}/games/deadbydaylight/shrine": {
