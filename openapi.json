                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "lockers": {
                        "type": "number",
                        "minimum": 0
                    },
                    "generator": {
                        "type": "string",
                        "enum": [
                            "possible",
                            "guaranteed",
                            "not_possible"
                        ]
                    },
                    "chest": {
                        "type": "string",
                        "enum": [
