                    "name": {
                        "type": "string"
                    },
                    "lockers": {
                        "type": "number",
                        "minimum": 0
                    },
                    "generator": {
                        "type": "string",
                        "enum": [
                            "possible",
                            "guaranteed",
                            "not_possible"
                        ]
                    },
                    "chest": {
                        "type": "string",
                        "enum": [
                            "possible",
                            "guaranteed",
                            "not_possible"
                        ]
                    },
                    "basement": {
                        "type": "string",
                        "enum": [
                            "possible",
                            "guaranteed",
                            "not_possible"
                        ]
                    },
                    "entrance_count": {
                        "type": "number",
                        "minimum": 0
                    }
                }
            },
