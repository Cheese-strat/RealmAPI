        "/{version}/games/deadbydaylight/info/generators": {
            "get": {
                "summary": "Returns information about generators",
                "description": "Generators are an interact-able Prop and the main Objectives of Survivors",
                "tags": [
                    "/games/deadbydaylight"
                ],
                "responses": {
                    "200": {
                        "description": "generator JSON object",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "age": {
                                            "type": "integer",
                                            "format": "int32",
                                            "minimum": 0
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/{version}/games/deadbydaylight/info/chests": {
            "get": {
                "summary": "Returns information about the chest prop",
                "description": "Optional extended description in CommonMark or HTML.",
                "tags": [
